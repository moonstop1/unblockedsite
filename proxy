<body>
  <button class="button-78" onclick="openGame()">Launch (PROXY NAME) #(PROXY LINK #)</button>
  <script>
    function openGame() {
      var win = window.open();
      var url = "PASTE-LINK-HERE";
      var iframe = win.document.createElement('iframe');
      iframe.style.width = "100%";
      iframe.style.height = "100%";
      iframe.style.border = "none";
      iframe.src = url;
      win.document.body.appendChild(iframe);
    }
  </script>
